//print all elements

#include <iostream>
#include <string>
#include <vector>

using namespace std;

void printAllElements(int n, vector<int> arr) {
	for (int i = 0; i < n; i++) {
		cout << "Item " << i + 1 << ": " << arr[i] << endl;
	}
}

int main()
{
	int n;
	cin >> n;
	vector<int> arr(n);
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}
	printAllElements(n, arr);
	return 0;
}


