//sum of all elements

#include <iostream>
#include <string>
#include <vector>

using namespace std;

int sumAllElements(int n, vector<int> arr) {
	int sum = 0;
	for (int i = 0; i < n; i++) {
		sum += arr[i];
	}
	return sum;
}

int main()
{
	int n;
	cin >> n;
	vector<int> arr(n);
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}

	int sum = sumAllElements(n, arr);

	cout << sum;
	return 0;
}
